{
  "name": "sentineldns",
  "private": true,
  "version": "0.9.3",
  "type": "module",
  "scripts": {
    "dev": "vite --config web/vite.config.ts",
    "build": "vite build --config web/vite.config.ts",
    "typecheck:frontend": "tsc -p web/tsconfig.frontend.json",
    "preview": "vite preview --config web/vite.config.ts",
    "screenshots": "node scripts/generate-screenshots.mjs",
    "smoke:compose": "node scripts/smoke-compose.mjs --compose-file deploy/compose/docker-compose.smoke.yml --dns-port 1053 --http-port 18080",
    "test:unit:frontend": "vitest run -c web/vitest.config.ts",
    "test:coverage:frontend": "vitest run -c web/vitest.config.ts --coverage",
    "test:unit:server": "npm --prefix server run test",
    "test:integration:server": "npm --prefix server run test:integration",
    "test:coverage:server": "npm --prefix server run test:coverage",
    "test:coverage": "npm run test:coverage:frontend && npm run test:coverage:server",
    "test:e2e": "playwright test -c tests/playwright.config.ts",
    "test:e2e:compose": "node scripts/e2e-compose.mjs",
    "test:ci": "node scripts/test-ci.mjs",
    "server:install": "npm --prefix server install",
    "server:dev": "npm --prefix server run dev",
    "server:build": "npm --prefix server run build",
    "server:start": "npm --prefix server run start"
  },
  "dependencies": {
    "@google/genai": "^1.39.0",
    "d3-geo": "^3.1.1",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "recharts": "^3.7.0",
    "topojson-client": "^3.1.0",
    "world-atlas": "^2.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.0.7",
    "autoprefixer": "^10.4.23",
    "jsdom": "^28.0.0",
    "playwright": "^1.50.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.0.7"
  }
}
